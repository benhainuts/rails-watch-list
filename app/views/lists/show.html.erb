<h1><%=@list.name%></h1>
<img src=<%=@list.image_url%> alt="">

<%@bookmarks.each do |bookmark|%>
  <li><%=Movie.find(bookmark.movie_id).title%> - <%=bookmark.comment%>
    <img src=<%=Movie.find(bookmark.movie_id).poster_url%> alt="">
  </li>
<%end%>

<p><%=link_to "create new bookmark", new_list_bookmark_path(@list)%></p>
<p><%=link_to "back", lists_path%></p>
